algoritmo "primeiro"
var
    //declarando variável
    ola, msg: Caractere;                                                  
inicio
    //atribuindo valor a variável
    ola <- "Olá, mundo!"
    msg <- "Me livrei da maldição."
    //mandando escrever utilizando variável
    escreval(ola)
    escreva(msg)
fimalgoritmo